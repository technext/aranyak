append content
    div.navbar.container.d-flex.w-100.justify-content-center.justify-content-lg-end.align-items-center

        for item in submenu
            a(href="#").d-flex.justify-content-center.align-items-center
                img(src=`${CWD}${item.imageSrc}`, alt="")
                span(style="margin-left:4px;").fs-10 #{item.label}
    div.bg-warning.mb-6.navbar-main
        div(style="padding: 12px 0px;").container.mx-auto.d-block.d-lg-flex.justify-content-between.align-items-center.text-center
            a(href="#")
                img(src=`${CWD}assets/img/logos/logo.png`, alt="")
            div
                input(type="search", name="search" placeholder=" search plants...").search.fs-8.bg-transparent

            for navMenu in menu 
                a(href=`${navMenu.link}`)
                     p.mb-0.fs-8.text-gray-900.fw-medium #{navMenu.label}